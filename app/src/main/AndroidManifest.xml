<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="ru.darvell.android.meetingclient"
		  android:versionCode="1"
		  android:versionName="1.0">

	<uses-sdk android:minSdkVersion="15"/>
	<application
			android:label="@string/app_name"
			android:icon="@drawable/ic_launcher"
			android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
		<activity
				android:name=".activitys.AuthActivity"
				android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name=".activitys.MainActivity"/>
		<activity android:name=".activitys.RegisterActivity"/>
		<activity android:name=".activitys.FriendsActivity"/>
		<service android:name=".service.MeetingService"></service>
	</application>
	<uses-permission android:name="android.permission.INTERNET"/>
</manifest>
